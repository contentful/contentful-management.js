{
	"compilerOptions": {
		"target": "esnext",
		"moduleResolution": "node",
		"allowJs": true,
		"noEmit": true,
		"outDir": "./typings/generated",
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"typeRoots": ["node_modules/@types", "global-types"]
	},
	"include": [
		"lib",
		"global-types"
	]
}
