{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "outDir": "./dist/typings",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitThis": false,
    "typeRoots": ["node_modules/@types"]
  },
  "include": ["lib", "global-types"],
  "typedocOptions": {
    "mode": "file",
    "out": "./out",
    "readme": "README.md",
    "name": "contentful-management.js",
    "exclude": ["./lib/adapters/REST/endpoints/*"],
    "includeVersion": true,
    "hideGenerator": true,
    "excludePrivate": true,
    "excludeNotExported": true,
    "ignoreCompilerErrors": true,
    "categorizeByGroup": true,
    "toc": [
      "createClient",
      "ClientParams",

      "ApiKey",
      "AppUpload",
      "AppDefinition",
      "AppInstalation",
      "AppBundle",
      "AppSignedRequest",
      "AppMetadata",
      "Asset",
      "AssetKey",
      "ContentType",
      "EditorInterface",
      "Entry",
      "Environment",
      "EnvironmentAlias",
      "Organization",
      "OrganizationInvitation",
      "OrganizationMembership",
      "PersonalAccessToken",
      "PreviewApiKey",
      "Role",
      "Snapshot",
      "Space",
      "SpaceMember",
      "SpaceMembership",
      "Team",
      "TeamSpaceMembership",
      "UIExtension",
      "Upload",
      "Usage",
      "User",
      "Webhooks"
    ]
  }
}
