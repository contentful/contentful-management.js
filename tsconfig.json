{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "outDir": "./typings/generated",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "typeRoots": ["node_modules/@types"]
  },
  "include": ["lib"],
  "typedocOptions": {
    "mode": "file",
    "out": "./out",
    "includeDeclarations": true,
    "excludeExternals": true,
    "readme": "README.md",
    "name": "contentful-management.js",
    "includeVersion": true,
    "hideGenerator": true,
    "excludePrivate": true,
    "ignoreCompilerErrors": true,
    "toc": [
      "createClient",
      "ClientAPI",
      "ContentfulSpaceAPI",
      "ContentfulEnvironmentAPI",
      "ContentfulOrganizationAPI",

      "ContentType",
      "Entry",
      "Asset",
      "ApiKey",
      "AppDefinition",
      "AppInstalation",
      "EditorInterface",
      "Environment",
      "EnvironmentAlias",
      "Organization",
      "OrganizationInvitation",
      "OrganizationMembership",
      "PersonalAccessToken",
      "PreviewApiKey",
      "Role",
      "Snapshot",
      "Space",
      "SpaceMember",
      "SpaceMembership",
      "Team",
      "TeamSpaceMembership",
      "UIExtension",
      "Upload",
      "Usage",
      "User",
      "Webhooks"
    ]
  }
}
